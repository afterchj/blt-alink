<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tpadsz.after.dao.PCFileDao">

    <select id="getFile" resultType="map" parameterType="map">
		select * from f_pc_file order by update_date desc limit 1
	</select>
    <insert id="saveFile" parameterType="map">
	    insert into f_pc_file(file_name,file_version,file_path,create_date,update_date) value (#{file_name},#{file_version},#{file_path},now(),now())
    </insert>
</mapper>